{
    "name": "nixes/pagescraper",
    "type": "library",
    "description": "A php library that attempts to grab the main contents of a news article. ",
    "keywords": ["article","news","scraper"],
    "homepage": "https://github.com/nixes/pagescraper",
    "license": "GPLv2",
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "friendsofphp/php-cs-fixer": "^3.1"
    },
    "autoload": {
        "psr-4": { "Nixes\\Pagescraper\\": "lib/" }
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml --coverage-text",
        "style": "php-cs-fixer fix lib"
    },
    "require": {
        "php": "^7.1.0",
        "ext-dom": "*",
        "ext-json": "*"
    }
}
